class Solution {
public:
    string defangIPaddr(string address) {
        string s2;
        for(int i=0;i<=address.length();i++){
            if(address[i]=='.'){
                s2=s2 + "[.]";
            }
            else{
                s2=s2+address[i];
            }
        }
        return s2;
    }
};
